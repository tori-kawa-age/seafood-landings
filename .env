# Postgres credentials (used for both Airflow metadata DB and project DB)
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflow
POSTGRES_HOST=localhost
POSTGRES_DB=seafood_landings

# Airflow metadata database (operational state only)
AIRFLOW_DB=airflow

# Project database for this repo (raw/staging/marts)
PROJECT_DB=seafood_landings

# Airflow Linux user inside containers (matches Airflow image default)
AIRFLOW_UID=50000

# Airflow Web UI user
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=admin
AIRFLOW_ADMIN_FIRSTNAME=Admin
AIRFLOW_ADMIN_LASTNAME=User
AIRFLOW_ADMIN_EMAIL=admin@example.com

# Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=seafood_landings
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
DBT_SCHEMA=seafood


# Shared Airflow secrets (must be identical across webserver/scheduler/init)
AIRFLOW__CORE__FERNET_KEY=${AIRFLOW__CORE__FERNET_KEY}
AIRFLOW__WEBSERVER__SECRET_KEY=dev_secret_key_change_me
